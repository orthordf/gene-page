{
  "name": "orth-genes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon app.js",
    "recreate-db": "rm -f data.sqlite3 && sequelize-cli db:migrate && NODE_OPTIONS=--max-old-space-size=16384 sequelize-cli db:seed:all"
  },
  "nodemonConfig": {
    "ignore": ["**/gene_data/**"],
    "delay": 2500
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.20",
    "papaparse": "^5.3.2",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.29.0",
    "sequelize-cli": "^6.6.0",
    "sqlite3": "5.0.2"
  }
}
